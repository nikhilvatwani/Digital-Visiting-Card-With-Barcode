<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.phpmysql.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity
            android:name=".DisplayContacts"
            android:label="@string/title_activity_display_contacts"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ReaderActivity"
            android:label="@string/title_activity_reader"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ChangingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AllContacts" />
        <activity
            android:name=".ActualContact"
            android:label="@string/title_activity_actual_contact"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>